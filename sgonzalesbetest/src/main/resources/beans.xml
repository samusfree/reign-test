<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.reign.sgonzales.betest.service.impl.IntegrationServiceImpl">
        <constructor-arg name="articleRepository" ref="articleRepository"/>
        <constructor-arg name="hackerNewsClient" ref="hackerNewsClient"/>
        <constructor-arg name="articleMapper" ref="articleMapper"/>
    </bean>
</beans>